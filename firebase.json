{
  "hosting": {
    "public": "public/mant", 
    "rewrites": [
      {
        "source": "/**",  
        "destination": "/index.html"
      }
    ],
    "headers": [
      {
        "source": "/**",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "no-store, no-cache, must-revalidate, proxy-revalidate"
          }
        ]
      }
    ],
    "errorPage": "/404.html"  
  }
}